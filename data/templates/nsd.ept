# nsd.conf

remote-control:
        control-enable: yes

server:
        hide-version: yes
		ip-address: 0.0.0.0
		ip-address: ::

<% for _, domain in ipairs (topLevelDomains or {}) do %>
zone:
        name: <%-domain:getDomainName() %>
        zonefile: "master/<%-domain:getDomainName() %>.zone"
<% end %>
